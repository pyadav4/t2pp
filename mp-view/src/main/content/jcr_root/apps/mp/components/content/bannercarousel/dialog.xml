<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <tags
                        jcr:primaryType="cq:Widget"
                        allowBlank="true"
                        disabled="false"
                        fieldLabel="Tags"
                        name="./cq:tags"
                        xtype="tags"/>
                    <requiredContent
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Required Pages"
                        name="./requiredcontent"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            xtype="pathfield"/>
                    </requiredContent>
                    <excludedContent
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Excluded Pages"
                        name="./excludedcontent"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            xtype="pathfield"/>
                    </excludedContent>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
